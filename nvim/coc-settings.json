{
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "python",
    "json",
    "rust",
    "sql"
  ],
  "python.jediEnabled": false,
  "explorer.width": 25,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.file.showHiddenFiles": true,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", ["expanded?", "collapse", "expand"], "open"],
    "o": "open",
    "v": "open:vsplit",
    "s": "open:split",
    "e": false
  },
  "snippets.textmateSnippetsRoots": ["/home/hawk/.config/snippets/"],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "coc.preferences.hoverTarget": "echo",
  "coc.preferences.jumpCommand": "vsplit",
  "diagnostic.errorSign": "●",
  "diagnostic.warningSign": "▲",
  "diagnostic.infoSign": "▲",
  "diagnostic.hintSign": "‣",
  "git.enableGutters": false,
  "snippets.priority": 100,
  "snippets.ultisnips.directories": ["UltiSnips", "gosnippets/UltiSnips"],
  "git.addedSign.text": "",
  "coc.preferences.extensionUpdateCheck": "daily",
  "yaml.schemas": {
    "kubernetes": "/*.yaml"
  },
  "styled-components.lint": {
    "showExpandedAbbreviation": "never"
  },
  "eslint.options": { "configFile": ".eslintrc.js" },
  "typescript.suggest.enabled": false
}
